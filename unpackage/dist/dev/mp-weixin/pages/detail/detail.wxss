
.contaioner.data-v-eca06f3c {
  padding-bottom: 65px;
}
.seat-group.data-v-eca06f3c {
  position: relative;
}
.chartDetaill.data-v-eca06f3c {
  color: #999;
  font-size: 14px;
}
.seatTable.data-v-eca06f3c {
  --size: calc((100vmin - 50px) / var(--col));
  width: calc(var(--size) * var(--col));
  display: flex;
  flex-wrap: wrap;
  background: #fff;
}
.refresh-btn.data-v-eca06f3c {
  margin-left: auto;
  align-self: flex-end;
  float: right;
  margin: 10px 2.5px;
}
.seat.data-v-eca06f3c {
  box-sizing: border-box;
  border-radius: 5px;
  border: 1px solid #cccccc;
  width: calc(var(--size) - 5px);
  height: calc(var(--size) - 5px);
  margin: 2.5px;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-size: cover;
  cursor: pointer;
  font-size: calc(50vmin / var(--size));
  color: #cccccc;
  display: flex;
  justify-content: center;
  align-items: center;
  overflow: hidden;
}
.seat.hide.data-v-eca06f3c {
  opacity: 0;
  pointer-events: none;
}
.seat.selected.data-v-eca06f3c {
  border: 2px solid #3292ff;
}
.btns.data-v-eca06f3c {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background: #fff;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.3);
  display: flex;
}
.btn.data-v-eca06f3c {
  margin: 0;
  z-index: 100;
  flex: 1;
  border-radius: 0;
}
.drawer.data-v-eca06f3c {
  width: 100vw;
  height: 100vh;
  position: fixed;
  background: #00000020;
  z-index: 200;
  inset: 0;
  transition: all 0.3s ease-in-out;
}
.drawer.hide.data-v-eca06f3c {
  opacity: 0;
  pointer-events: none;
}
.drawer.hide .drawer-content.data-v-eca06f3c {
  transform: translateY(100%);
}
.drawer-content.data-v-eca06f3c {
  transition: all 0.3s 0.3s;
  background: #fff;
  position: absolute;
  bottom: 0;
  padding: 10px;
  width: calc(100% - 20px);
  border-radius: 10px 10px 0 0;
}
.title.data-v-eca06f3c {
  font-size: 18px;
  margin-bottom: 5px;
  margin-left: 10px;
}
.roomInfo.data-v-eca06f3c,
.userinfo.data-v-eca06f3c {
  color: #666;
}
.drawer button.data-v-eca06f3c {
  margin: 0 10px;
